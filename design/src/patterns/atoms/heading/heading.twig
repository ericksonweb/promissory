{#
/**
 * Heading Pattern
 */
#}
{% extends '@promissory/_macros/_element.twig' %}

{% set level = level|default('1') %}
{% if level in range(1, 6) %}
  {% set element = 'h' ~ level|default('1') %}
  
  {% block content %}
    {{- content|default("Heading Level " ~ level|default('1')) -}}
  {% endblock %}
    
{% endif %}
